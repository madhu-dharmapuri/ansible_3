pipeline {
      
      agent any 
      
      stages {
      
      stage (build) {
          steps {
          echo 'Build the file'
          }
     
      }
      
      stage (test) {
          steps {
          echo 'Test the file'
          }
      
      }
      
      
      stage (deploy) {
          steps {
          echo 'Deploy the file'
          }
      
      }
      
      
      }
      
}
